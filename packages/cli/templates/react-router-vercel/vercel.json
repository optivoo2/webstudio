{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "react-router",
  "build": {
    "env": {
      "NODE_ENV": "production",
      "VERCEL": "1",
      "NODE_OPTIONS": "--max-old-space-size=6144",
      "SKIP_GLOBAL_ASSIGN_CHECK": "1"
    }
  },
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
