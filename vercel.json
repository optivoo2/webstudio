{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm --filter='@webstudio-is/builder' build",
  "installCommand": "pnpm install --no-frozen-lockfile --force",
  "outputDirectory": "apps/builder/build",
  "framework": "remix",
  "build": {
    "env": {
      "ENABLE_VC_BUILD": "1",
      "NODE_ENV": "production",
      "VERCEL": "1",
      "NODE_OPTIONS": "--max-old-space-size=6144",
      "SKIP_GLOBAL_ASSIGN_CHECK": "1"
    }
  },
  "git": {
    "deploymentEnabled": true
  },
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
